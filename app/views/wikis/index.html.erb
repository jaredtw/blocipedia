<h1>Public Wikis</h1>

<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
       <%= render partial: 'wiki', locals: { wiki: wiki } %>
    <% end %>
  </div>

  <div class="col-md-4">
    <%= link_to "New Public Wiki", new_wiki_path, class: 'btn btn-success' %>
  </div>
</div>
